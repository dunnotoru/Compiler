    <v:String x:Key="about">Über</v:String>
    <v:String x:Key="settings">Einstellungen</v:String>

    <!-- Ausgabe-Raster -->
    <v:String x:Key="file_name">Dateiname</v:String>
    <v:String x:Key="line">Zeile</v:String>
    <v:String x:Key="column">Spalte</v:String>
    <v:String x:Key="message">Nachricht</v:String>

    <v:String x:Key="code">Code</v:String>
