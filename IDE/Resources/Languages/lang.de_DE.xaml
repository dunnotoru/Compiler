    <v:String x:Key="token_identifier">Bezeichner</v:String>
    <v:String x:Key="token_integer">Ganzzahl</v:String>
    <v:String x:Key="token_double">Kommazahl</v:String>
    <v:String x:Key="token_string">Zeichenkette</v:String>

    <v:String x:Key="token_whitespace">Leerzeichen</v:String>
