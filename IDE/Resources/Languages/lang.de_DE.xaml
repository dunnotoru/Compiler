    <v:String x:Key="open">Öffnen</v:String>
    <v:String x:Key="save">Speichern</v:String>
    <v:String x:Key="save_as">Speichern als</v:String>
    <v:String x:Key="exit">Beenden</v:String>

    <v:String x:Key="edit">Bearbeiten</v:String>
    <v:String x:Key="undo">Rückgängig</v:String>
    <v:String x:Key="redo">Wiederholen</v:String>
    <v:String x:Key="copy">Kopieren</v:String>
    <v:String x:Key="cut">Ausschneiden</v:String>
    <v:String x:Key="paste">Einfügen</v:String>
    <v:String x:Key="remove">Entfernen</v:String>
    <v:String x:Key="select_all">Alles auswählen</v:String>

    <v:String x:Key="text">Text</v:String>
