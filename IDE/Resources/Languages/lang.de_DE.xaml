    <v:String x:Key="token_lessorequal">Kleiner oder gleich</v:String>
    <v:String x:Key="token_equal">Gleich</v:String>
    <v:String x:Key="token_and">Und</v:String>
    <v:String x:Key="token_or">Oder</v:String>
    <v:String x:Key="token_not">Nicht</v:String>
    <v:String x:Key="token_true">Wahr</v:String>
    <v:String x:Key="token_false">Falsch</v:String>

    <v:String x:Key="token_invalid">Ungültig</v:String>

</ResourceDictionary>