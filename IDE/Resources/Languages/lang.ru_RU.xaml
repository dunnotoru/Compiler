    <v:String x:Key="startIndex">Начальная позиция</v:String>
    <v:String x:Key="endIndex">Конечная позиция</v:String>

    <!-- settings -->
    <v:String x:Key="settings_back">Назад</v:String>
    <v:String x:Key="settings_language">Язык</v:String>

    <!-- messages -->
    <v:String x:Key="msg_save_changes">Сохранить изменения?</v:String>
</ResourceDictionary>