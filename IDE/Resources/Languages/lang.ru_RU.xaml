    <v:String x:Key="show_help">Просмотр справки</v:String>
    <v:String x:Key="about">О программе</v:String>
    <v:String x:Key="settings">Настройки</v:String>

    <!-- output grid -->
    <v:String x:Key="file_name">Имя файла</v:String>
    <v:String x:Key="line">Строка</v:String>
    <v:String x:Key="column">Символ</v:String>
